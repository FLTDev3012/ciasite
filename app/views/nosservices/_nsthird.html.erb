<script>

  window.addEventListener('scroll', function() {
      var scrollPosition = window.scrollY;
      console.log(scrollPosition);




      var scrollNsThirdOnePic = document.querySelector('.js-ns-third-one-pic');

      if (scrollPosition > 1100) {
        scrollNsThirdOnePic.classList.add('show');
      } else {
        scrollNsThirdOnePic.classList.remove('show');
      }

      var scrollNsThirdTwoPic = document.querySelector('.js-ns-third-two-pic');


            <div class="text-bold"><%= t('services.nsseconde.case_choice.ecommerce') %></div>
          </div>
        </div>

        <div class="ns-stimulus-gauche-oneline" data-action="click->critere#update5" data-critere-target="cinquiemecrittitle">

            <div class="link-ligne-rouge-red"></div>
            <div class="text-bold"><%= t('services.nsseconde.case_choice.showcase') %></div>
          </div>
        </div>

        <div class="ns-stimulus-gauche-oneline" data-action="click->critere#update3" data-critere-target="troisiemecrittitle">

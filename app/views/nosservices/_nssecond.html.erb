      <h2 class="text-title"><%= t('services.nsseconde.title') %></h2>

      <div class="ns-stimulus-gauche">

        <div class="ns-stimulus-gauche-oneline opacity-one" data-action="click->critere#update1" data-critere-target="premiercrittitle">
